# Database Configuration
# URL to download the database file from (e.g., Dropbox, Google Drive, AWS S3)
# Leave empty if database file is already present locally
DATABASE_URL=

# Alpaca Broker API Configuration (OAuth)
# Client ID from Alpaca Broker API
ALPACA_CLIENT_ID=your_client_id_here

# Private key (base64-encoded, WITHOUT the BEGIN/END delimiters)
# Copy only the content between -----BEGIN PRIVATE KEY----- and -----END PRIVATE KEY-----
ALPACA_PRIVATE_KEY=your_base64_private_key_here

# Alpaca API endpoint
ALPACA_BASE_URL=https://broker-api.sandbox.alpaca.markets

# Server Configuration
PORT=3000
